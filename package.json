{
  "name": "freesearch-mcpserver",
  "version": "1.0.0",
  "description": "Free Search MCP Server - A Model Context Protocol server for web search using DuckDuckGo",
  "main": "dist/index.js",
  "bin": {
    "freesearch-mcpserver": "dist/mcp-stdio.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "mkdir -p dist/config && cp -r src/config/* dist/config/",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:rpc": "node test-rpc.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "search",
    "duckduckgo",
    "web-search",
    "ai",
    "llm",
    "cursor",
    "claude"
  ],
  "author": {
    "name": "frank_123go"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/freesearch-mcpserver.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/freesearch-mcpserver/issues"
  },
  "homepage": "https://github.com/your-username/freesearch-mcpserver#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/commander": "^2.12.0",
    "@types/express": "^5.0.3",
    "@types/handlebars": "^4.0.40",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.13",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.4",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "jsonrpc-lite": "^2.2.0",
    "zod": "^3.25.76"
  }
}
